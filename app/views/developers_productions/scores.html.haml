=render 'links'

%section#filter.mt-3
  =render 'filter_scores'

%hr/
%section#developers_production.small
  .row.text-center.justify-content-center
    .col-2 developer
    .col-2 hours
    .col-2 score
  %hr/
  -@rows_reports.each do |row|
    -puts row[:developer].first_name
    %hr/
    .row.text-center.justify-content-center
      .col-2
        ="#{row[:developer].first_name} #{row[:developer].last_name}"
      .col-2
        =row[:hours]
      .col-2
        =row[:score]


