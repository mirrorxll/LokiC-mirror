.row.mb-1.font-weight-bold
  .col-4
    level
  .col-4
    frequency
  .col-4
    photo bucket
.row
  -[@story_type&.level&.name, @story_type&.frequency&.name, @story_type&.photo_bucket&.name].each do |name|
    .col-4
      -if name
        =name
      -else
        .text-danger.font-italic
          not set
-if @story_types_permissions['developer']['show']
  .row.mb-1.font-weight-bold.mt-3
    .col
      developer
  #developer_section
    =render 'story_types/properties/developer'
