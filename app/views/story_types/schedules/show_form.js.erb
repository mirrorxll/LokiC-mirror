form_schedule = $('#forms_schedule')
<% if @type == 'manual'%>
  form_schedule.html('<%= j render 'story_types/schedules/form' %>')
  $('#add_form').click()
<% elsif @type == 'auto'%>
  form_schedule.html('<%= j render 'story_types/schedules/form_auto' %>')
<% elsif @type == 'backdate'%>
  form_schedule.html('<%= j render 'story_types/schedules/form_backdate', iteration: @iteration %>')
<% end %>
