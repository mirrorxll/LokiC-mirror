#report_tracking_hour
  .nav
    .nav-link.active
      =link_to 'Week1', tracking_hours_path, params: weeks.last
    .nav-link Week2
    .nav-link Week3
  =render 'tracking_hours/table', rows_reports: @rows_reports
#forms.row.small
  =render 'tracking_hours/form'
.row.pt-3
  .col-6
    =button_to('Add row', add_form_tracking_hours_path, remote: true)
  .col-6
    =button_to('Submit', class: 'btn btn-success', onclick: "document.getElementsByTagName('report_button').submit();")

-#.row.pt-1
