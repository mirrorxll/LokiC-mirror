$('#task_status').html(`<%= j render 'task_statuses/status', task: @task %>`);
$('#done').html(`<%= j render 'tasks/done', task: @task %>`);
<%-unless %w(blocked canceled).include? @task.status.name %>
  $('#comments').prepend(`<%= j render 'task_comments/comment', comment: @comment %>`);
<%-end %>
