%div{id: "story_type_#{story_type.id}_developer"}
  =form_with(url: include_story_type_developers_path(story_type),
             method: :patch, html: { onchange: "Rails.fire(this, 'submit')" }) do |form|
    =form.collection_select :developer_id, Account.all, :id, :name, { prompt: '' }, { name: :id }
