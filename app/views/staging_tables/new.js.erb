var st_table_block = $('#staging_table .card-body')
<% if flash.now[:error] %>
  st_table_block.prepend(`
      <div class="alert alert-danger">
        <%= flash.now[:error].gsub('`', "'") %>
      </div>`)
<% else %>
  st_table_block.empty()
      .html('<%= j render 'staging_tables/form'%>')
  $('#samples .card-body').empty()
  $('#scheduler .card-body').empty()
  $('#export .card-body').empty()
<% end %>
