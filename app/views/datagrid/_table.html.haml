-# Local variables:
-# grid - instance of Datagrid
-# assets - Array of database Entities
-# options - passed options Hash

-case grid
-when AccountsGrid
  =render 'datagrid/accounts/table', grid: grid, assets: assets, options: options
-when WorkRequestsGrid
  =render 'datagrid/work_requests/table', grid: grid, assets: assets, options: options
-when FactoidRequestsGrid
  =render 'datagrid/factoid_requests/table', grid: grid, assets: assets, options: options
-when StoryTypesGrid
  =render 'datagrid/story_types/table', grid: grid, assets: assets, options: options
-when DataSetsGrid
  =render 'datagrid/data_sets/table', grid: grid, assets: assets, options: options
-when FactoidTypesGrid
  =render 'datagrid/factoid_types/table', grid: grid, assets: assets, options: options
